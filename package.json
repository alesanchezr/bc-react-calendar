{
  "name": "bc-react-calendar",
  "version": "1.0.0",
  "description": "Calendar component for react",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "start-storybook -p 3000 --ci",
    "build-storybook": "build-storybook",
    "deploy": "build-storybook && node deploy-github.js",
    "publish-to-npm": "webpack --config webpack.config.js --mode production && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alesanchezr/bc-react-calendar.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [
    "React",
    "BreatheCode"
  ],
  "author": "Alejandro Sanchez",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/alesanchezr/bc-react-calendar/issues"
  },
  "homepage": "https://github.com/alesanchezr/bc-react-calendar#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.9",
    "@storybook/addon-info": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.9",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-notes": "^5.0.9",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.9",
    "babel-loader": "^8.0.0-beta.6",
    "gh-pages": "^2.0.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.0.3"
  },
  "dependencies": {
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dnd": "9.2.1",
    "react-dnd-html5-backend": "9.1.0",
    "react-dom": "16.8.6",
    "styled-components": "5.0.0-beta.6-ej4"
  }
}
